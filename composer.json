{
  "name": "nahid/literature",
  "description": "A fun project for making code poetry",
  "keywords": ["code", "poetry", "literature"],
  "homepage": "https://github.com/nahid/literature",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Nahid Bin Azhar",
      "email": "nahid.dns@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.5.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8 || ^5.0",
    "symfony/var-dumper": "^3.4",
    "graham-campbell/testbench": "^3.1"
  },
  "autoload": {
    "psr-4": {
      "Nahid\\Literature\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Nahid\\JsonQ\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}